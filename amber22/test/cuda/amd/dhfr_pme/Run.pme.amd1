#!/bin/csh -f
#TEST-PROGRAM pmemd.cuda
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

#$1 = PREC_MODEL
#$2 = NETCDF

if( ! $?DO_PARALLEL ) then
  setenv DO_PARALLEL " "
  if( $?TESTsander ) then
      set sander = $TESTsander
  else
      set sander = ${AMBERHOME}/bin/pmemd.cuda_$1
  endif
else
  if( $?TESTsander ) then
      set sander = $TESTsander
  else
      set sander = ${AMBERHOME}/bin/pmemd.cuda_$1.MPI
  endif
endif

cat > mdin <<EOF
 short md, nve ensemble test AMD=1 PME
 &cntrl
  imin=0,irest=1,ntx=5,
  nstlim=25,dt=0.002,
  ntc=2,ntf=2,tol=0.000001,
  temp0=300.0,iwrap=1,
  cut=8.0, ntb=1,
  ntpr=1, ntwx=1,ntwr=1,
  ntt=0,ioutfm=0,ig=2221,ntave=0, 
   iamd=1,
   ethreshp=-67144,alphap=4586,
   ethreshd=1088.3,alphad=111.3, ig=71277,
 /
 &ewald
  dsum_tol=0.000001,nfft1=96,
  nfft2=96,nfft3=96,netfrc=0
 /
EOF

set output = mdout.pme.amd1

set output_save = $output.GPU_$1

touch dummy
$DO_PARALLEL $sander -O -i mdin -c md12.x -o $output <dummy || goto error
#Use different dacdif tolerances based on precision model.
if ( "$1" == "DPFP" ) then
  #7 sig figs
  ../../../dacdif -r 1.0e-7 $output_save $output
else
  #4 sig figs
  ../../../dacdif -r 3.1e-4 $output_save $output
endif

/bin/rm -f mdin logfile mdinfo dummy mdcrd restrt amd.log
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)








