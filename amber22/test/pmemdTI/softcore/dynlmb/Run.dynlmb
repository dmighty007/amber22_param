#!/bin/csh -f
#TEST-PROGRAM pmemd
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented
 
if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/pmemd"
 
if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin.0 <<EOF
NPT production
 &cntrl
  imin = 0,	ntx = 5,	irest = 1,
  ntpr = 10,	ntwr = 0,	ntwx = 0, ioutfm=0, ntxo=1,
  ntave = 10,
  ntf = 1,	ntc = 2,
  ntb = 2,	cut = 9.0,
  nstlim = 90,	dt = 0.002,
  temp0 = 300.0,	ntt = 3,	gamma_ln = 2,
  ntp = 1,	pres0 = 1.0,	taup = 2.0,
  icfe=1,       clambda = 0.10000,
  ifsc=1,       timask1=':1', timask2='',
                scmask1=':1', timask2='',
  logdvdl=1,	dynlmb = 0.1, ig=71277,
 /
EOF

cat > groupfile <<EOF
-O -i mdin.0 -o out.0 -p ../prmtop.0 -c ../rst.0 -r restrt.0
EOF

$DO_PARALLEL $TESTsander `cat groupfile` < /dev/null || goto error

if ( `basename $TESTsander` == "pmemd.mic_offload.MPI" ) then
../../../dacdif -r 1.e-7 out.0.save out.0
../../../dacdif -r 1.e-3 restrt.0.save restrt.0
else
../../../dacdif out.0.save out.0
../../../dacdif -r 1.e-5 restrt.0.save restrt.0
endif

/bin/rm -f mdin.0 mdinfo groupfile logfile
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)







