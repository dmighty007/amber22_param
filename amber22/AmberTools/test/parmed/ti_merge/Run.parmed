#!/bin/sh

. ../../program_error.sh

if [ -z "$parmed" ]; then

   parmed="$AMBERHOME/bin/parmed"
fi

output=parmed.out
prmtop=abs.prmtop

$parmed -O $prmtop parmed.in > $output 2>&1 || error

../../dacdif -a 1e-5 abs_merged.inpcrd.save abs_merged.inpcrd
../../dacdif abs_merged.prmtop.save abs_merged.prmtop

/bin/rm -f $output

exit 0
