#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
    set sander = $TESTsander
endif

if( ! $?DO_PARALLEL ) then
    setenv DO_PARALLEL " "
endif

../../../check_slko_files.x
if( $status > 0) then
  exit(0)
endif

cat > mdin <<EOF
 Ala8, 5 Md steps
 &cntrl
   imin=0, 
   irest=1, ntx=5,
   ntpr=1,
   nstlim=5, dt=0.002,
   ntc=2, ntf=2,
   ntt=1, temp0=300, tempi=300.0,
   igb=0, ntb=0, cut=99.0,
   ifqnt=1,
 /

 &qmmm
   qmmask='@*',
   qm_theory='DFTB' ,
   dftb_3rd_order='PR',
   tight_p_conv=1,
 /
EOF

set output = mdout_PR

touch dummy
$DO_PARALLEL $sander -O -o $output < dummy || goto error
../../../dacdif -t 1 $output.save $output

/bin/rm -f restrt mdin mdinfo dummy CUBE.DAT fort.7
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)







