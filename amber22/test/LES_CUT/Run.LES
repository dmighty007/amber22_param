#!/bin/csh -f
#TEST-PROGRAM sander.LES
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsanderLES ) set TESTsanderLES = "${AMBERHOME}/bin/sander.LES"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

echo " "
echo "  SANDER.LES test, no PME "
echo " "

$DO_PARALLEL $TESTsanderLES  -O \
		  -i md.in \
		  -o md.LES.out \
		  -p LES.prmtop \
       	  -c LES.crd \
		  -r md.LES.rst 		|| goto error

../dacdif md.LES.out.save  md.LES.out

/bin/rm -f  md.LES.rst mdinfo 
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
