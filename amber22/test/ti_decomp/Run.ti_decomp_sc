#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/sander.MPI"

if( ! $?DO_PARALLEL ) then
   echo "This test must be run in parallel --skipping"
   exit(0)
else
	set numprocs=`echo $DO_PARALLEL | awk -f ../numprocs.awk `
	if ( "$numprocs" > 2 ) then
	    echo " DO_PARALLEL set to $DO_PARALLEL"
	    goto toomany
	else if ( $?MP_PROCS)then
	    if ($MP_PROCS > 2)then
#               --- For IBM AIX ---
		echo "    MP_PROCS set to "$MP_PROCS
		goto toomany
	    endif
	endif
endif

cat > mdin <<EOF
         thermodynamic integration with decomposition of dV/dL on per residue level
 &cntrl
  nmropt = 0,

  ntx    = 5,       irest  = 1,       ntrx   = 1,      ntxo   = 1,
  ntpr   = 2,       ntwx   = 0,       ntwv   = 0,      ntwe   = 0,
  ioutfm = 0,       ntwprt = 0,       icfe   = 1,      clambda= 0.50000,
  klambda= 1,       idecomp= 1,       ifsc   = 1,
  noshakemask = '@199,545', scmask = '@199,545',

  ntf    = 1,       ntb    = 1,       dielc  = 1,      igb    = 0,
  cut    = 8.0,     nsnb   = 10,

  imin   = 0,
  nstlim = 10,
  nscm   = 0,
  t      = 200.0,   dt     = 0.002,

  temp0  = 300.0,   tempi  = 300.0,
  ntt    = 1,
  tautp  = 5.0,
  vlimit = 20.0,

  ntp    = 0,       pres0  = 1.0,     comp   = 44.6,
  taup   = 0.5,

  ntc    = 2,       tol    = 0.000001,

 /
 &ewald
  order  = 4,        ew_type = 0,
  dsum_tol= 0.00001, use_pme = 1,
 /
Residues considered for decomp
RRES 1 6228
END
Residues to print
RES 1 6228
END
END
 /
EOF

cat > groupfile <<EOF
-O -i mdin -o ti_sc_decomp_1.out -p F.prmtop -c restrt -r restrt_1
-O -i mdin -o ti_sc_decomp_2.out -p H.prmtop -c restrt -r restrt_2
EOF

$DO_PARALLEL $TESTsander -ng 2 -groupfile groupfile < /dev/null || goto error

../dacdif -r 1.0e-5 ti_sc_decomp_1.out.save ti_sc_decomp_1.out
../dacdif -r 1.0e-5 ti_sc_decomp_2.out.save ti_sc_decomp_2.out
/bin/rm -f mdin restrt_? mdinfo groupfile 
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)

toomany:
    echo " too many processors for this test, exiting"
    echo "============================================================"
    exit(0)










