#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/sander.APBS"

if( ! $?DO_PARALLEL ) then
   setenv DO_PARALLEL " "
else
   echo "This test not set up for parallel"
   echo " cannot run in parallel with iAPBS"
   exit 0
endif

setenv MCSH_HOME /dev/null

/bin/rm -f mdin

cat > mdin <<EOF
 test of iAPBS/sander solvation energies (based on pbsa_radi)
 &cntrl 
   ntx=1, irest=0,
   imin=1, maxcyc=0,
   ntpr=1, ntwr=100000,
   igb=6, ntb=0, cut = 8.0,
   ntc=1, ntf=1, tol=0.000001,
   ntt=0, temp0=300
 &end
 &apbs
    apbs_debug=0,
    apbs_print=0,
    grid=0.5, 0.5, 0.5,
    calc_type=0,
    cmeth=1,
    bcfl=2,
    srfm=1,
    chgm=1,
    pdie=1.0,
    sdie=80.0,
    srad = 1.4,
    radiopt=0,
    calcenergy=1, calcforce=0, calcnpenergy=1, calcnpforce=0,
&end
EOF

set all = "tp3 ile leu val phe trp asp cym glu hip lys"
foreach aa ($all)
   set output = $aa.out

   $DO_PARALLEL $TESTsander -O -i mdin -p ./prmtop-inpcrd/$aa.prmtop -c ./prmtop-inpcrd/$aa.inpcrd  -o $output < /dev/null || goto error

   ../dacdif -t 1 ./out.save/$aa.out.save $output
   /bin/rm -f restrt mdinfo mdcrd
   goto next

   error:
   echo "  ${0}:  Program error"
   exit(1)
  
   next:
end









