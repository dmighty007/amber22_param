#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
   set sander = $TESTsander
endif

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

set output = mdout.ff14ipq

cat > mdin <<EOF
 short md, nve ensemble
 &cntrl
   ntx=5, irest=1,
   ntc=2, ntf=2, tol=0.0000001, 
   nstlim=20, ntt=0, 
   ntpr=1, ntwr=10000, 
   dt=0.001, vlimit=10.0, ig=71277,
 /
 &ewald
   dsum_tol = 5.0e-6,
   nfft1 = 64, nfft2 = 64, nfft3 = 64,
 /
EOF

$DO_PARALLEL $sander -O -i mdin -c eq10.x -o $output < /dev/null || goto error
if ( `basename $sander` == "pmemd.mic_offload.MPI" ) then
../dacdif -r 1.e-05 $output.save $output
else
../dacdif -r 1.e-06 $output.save $output
endif
/bin/rm -f mdin restrt mdinfo
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









