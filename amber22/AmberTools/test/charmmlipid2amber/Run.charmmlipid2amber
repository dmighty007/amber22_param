#!/bin/sh

. ../program_error.sh
if [ -z "$charmmlipid2amber" ]; then

   charmmlipid2amber="$AMBERHOME/bin/charmmlipid2amber.py"
fi
/bin/rm -f tmp charmmlipid2amber.out

# List of test files
file_list="./input/B2AR.pdb
./input/chl1.pdb
./input/dapc.pdb
./input/dlpc.pdb
./input/dmpc.pdb
./input/dopc.pdb
./input/dppc.pdb
./input/dspc.pdb
./input/plpc.pdb
./input/popa.pdb
./input/popc.pdb
./input/pope.pdb
./input/popg.pdb
./input/pops.pdb
./input/sdpc.pdb"

for file in $file_list; do
  $charmmlipid2amber -i $file -o tmp || error
  cat tmp >> charmmlipid2amber.out
  /bin/rm -f tmp
done
../dacdif charmmlipid2amber.out.save charmmlipid2amber.out
/bin/rm -f tmp charmmlipid2amber.out
exit 0
