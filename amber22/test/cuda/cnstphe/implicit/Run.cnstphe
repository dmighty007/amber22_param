#!/bin/csh -f
#TEST-PROGRAM pmemd.cuda
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

#$1 = PREC_MODEL
#$2 = NETCDF

if( ! $?DO_PARALLEL ) then
  setenv DO_PARALLEL " "
  if( $?TESTsander ) then
      set sander = $TESTsander
  else
      set sander = ${AMBERHOME}/bin/pmemd.cuda_$1
  endif
else
  if( $?TESTsander ) then
      set sander = $TESTsander
  else
      set sander = ${AMBERHOME}/bin/pmemd.cuda_$1.MPI
  endif
endif

cat > mdin <<EOF
 Test of constant pH,E in GB
 &cntrl  
   ig=49,
   nstlim = 25
   cut=1000.0, igb=2, saltcon=0.1,
   ntx=1, irest=0, ntb=0, ntpr=1,
   dt=0.002, nrespa=1,
   ntt=1, tempi=300.0, temp0 = 300., tautp=2.0,
   ntc=2, ntf=2, tol=0.000001,
   ntwr = 20, 
   icnste=1, ntcnste=2, solve=-0.23,
   icnstph=1, ntcnstph=2, solvph=5.0,
/
EOF

set output_save = mdout.GPU_$1
set cpoutput_save = cpout.GPU_$1
set ceoutput_save = ceout.GPU_$1

touch dummy
$DO_PARALLEL $sander -O -c min.x < dummy || goto error
#Use different dacdif tolerances based on precision model.
if ( "$1" == "DPFP" ) then
  #7 sig figs
  ../../../dacdif -r 1.0e-7 $output_save mdout
else
  #5 sig figs
  ../../../dacdif -r 1.0e-5 $output_save mdout
endif
../../../dacdif $cpoutput_save cpout
../../../dacdif $ceoutput_save ceout

/bin/rm -f mdin logfile mdinfo dummy restrt mdcrd cprestrt cerestrt
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)








