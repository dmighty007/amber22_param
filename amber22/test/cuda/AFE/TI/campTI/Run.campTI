#!/bin/csh -f
#TEST-PROGRAM pmemd.cuda
#TEST-DESCRIP non-softcore TI
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

#$1 = PREC_MODEL
#$2 = NETCDF

#if( $?TESTsander ) then
#    set sander = $TESTsander
#else
    set sander = ${AMBERHOME}/bin/pmemd.cuda_$1
#endif


cat > mdin <<EOF
morph rpcamp into camp charges and S -> O
 &cntrl
   ntr=0,
   nstlim =10, nscm=2000, ntave=5000,
   ntx=1, irest=0, ntb=2, ntpr=1, tempi=300.0, ig=974651,
   ntp=1, taup=1.0,
   barostat = 2, mcbarint = 5,
   dt=0.001, nrespa=1,
   ntt=3, gamma_ln = 2,
   temp0 = 300., tautp=2.0,
   ntc=2, ntf=2, tol=0.000001,
   ntwr = 10000, ntwx=0,
   icfe=1, clambda=0.5,
   timask1=':1',
   timask2=':2',
   cut=9.0,
 /
EOF

set output = campTI.out

set output_save = $output.$1

touch dummy
$sander -O -i mdin -c inpcrd -p ti.prmtop -o $output <dummy || goto error

#Use different dacdif tolerances based on precision model.
if ( "$1" == "DPFP" ) then
  #7 sig figs
  ../../../../dacdif -r 1.0e-7 $output_save $output
else
  #5 sig figs
  ../../../../dacdif -r 1.0e-5 $output_save $output
endif

/bin/rm -f mdin restrt mdinfo dummy
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)







