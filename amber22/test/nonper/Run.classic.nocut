#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > gbin <<EOF
short classic md run no frills really big cutoff
 &cntrl 
   nstlim = 20,
   ntpr   = 1,  
   cut=99.0, 
   ntwr=100000, ntwx=1, 
   ntx=5, irest=1, 
   ntc=2, ntf=2, tol=0.000001,
   dt=0.002, ntb=0,
   ibelly=0,
   tempi=300,temp0=300,tautp=.5,
   ntt=0,

   imin=0, maxcyc=10,ncyc=100
 /
EOF

$DO_PARALLEL sander_classic -O -i gbin -c inpcrd -o mdout < /dev/null || goto error

../dacdif mdout.classic_nocut.save mdout
/bin/rm -f gbin restrt mdinfo
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









