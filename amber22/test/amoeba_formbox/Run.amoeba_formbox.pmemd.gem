#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/gem.pmemd"
if( $?TESTsander ) then
   set sander = $TESTsander
endif

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin <<EOF
 short md, nve ensemble
 &cntrl
   ntx=7, irest=1,
   nstlim=10,
   ntpr=1, ntwr=1000,ntave=1000, 
   nscm=1000,ntwx=0,
   dt=0.001, vlimit=10.0,
   cut=8.,maxcyc=50,ntmin=2,imin=0,
   ntt=1, temp0=298.0,tempi=0.0,tautp=1.0,
   ntp=0, iamoeba=1,
 /
 &ewald
  nfft1=24,nfft2=24,nfft3=24,
  skinnb=0.3,nbtell=0,order=5,ew_coeff=0.45,
 /
 &amoeba
   do_self=1,dipole_scf_tol = 0.01,dipole_scf_iter_max=30,
   sor_coefficient=0.7,ee_damped_cut=4.5,ee_dsum_cut=6.7,
 /
EOF

set output = "amoeba_formbox.pmemd.gem.mdout"

$DO_PARALLEL $sander -O -c inpcrd.rst7 -o $output || goto error

../dacdif ${output}.save ${output}
/bin/rm -f mdin restrt mdinfo logfile
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
