#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
   set sander = $TESTsander
endif

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > gbin <<EOF
 short md, nve ensemble, truncated octahedron, nrespa=2
 &cntrl
   ntx=5, irest=1,
   ntc=2, ntf=2, tol=0.0000001, 
   nstlim=10, ntt=0, 
   ntpr=2, ntwr=10000, 
   dt=0.001, vlimit=10.0, nrespa=2, ig=71277,
 /
EOF

touch dummy
$DO_PARALLEL $sander -O -i gbin -o mdout.trx < dummy || goto error

../dacdif mdout.trx.save mdout.trx
/bin/rm -f gbin restrt mdinfo dummy
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
