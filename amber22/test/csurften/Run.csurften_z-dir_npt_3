#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
   set sander = $TESTsander
endif

if( ! $?DO_PARALLEL ) then
	setenv DO_PARALLEL " "
endif

cat > mdin <<EOF
short md, npt ensemble constant surf ten z dir
 &cntrl
   ntx=5, irest=1,
   ntc=2, ntf=2,
   nstlim=20, ntt=1, tautp = 10.0,
   temp0=300.0,
   ntb=2, ntp=3, taup=1.0,
   csurften=3, gamma_ten=60.0,
   ninterface = 2,
   ntpr=1, ntwr=10000, ntwx=0,
   dt=0.002, ig=71277,
 /
EOF
$DO_PARALLEL $sander -O -p dppc_128.prmtop -o mdout.csurften_z-dir_npt_3 || goto error

if ( `basename $sander` == "pmemd.mic_offload.MPI" ) then
../dacdif -r 1.0e-05 mdout.csurften_z-dir_npt_3.save mdout.csurften_z-dir_npt_3
else
../dacdif mdout.csurften_z-dir_npt_3.save mdout.csurften_z-dir_npt_3
endif
/bin/rm -f mdin restrt mdinfo
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









