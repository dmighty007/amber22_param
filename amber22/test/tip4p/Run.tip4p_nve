#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
   set sander = $TESTsander
endif


if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin <<EOF
 equilibration, polarizable solute
 &cntrl
  irest  = 1, ntx    = 5,
  ntb    = 1, ntp    = 0, taup=0.5,
  cut    = 6.0,
  ntf=2, ntc=2, tol=0.000001,
  nstlim=40, ntpr=1,
  ntt=1, temp0=300., tautp=0.5,
  dt=0.001, 
  jfastw=1, watnam='TP4', ig=71277,
 /
 &ewald
   skinnb=1.0, nfft1=18, nfft2=18, nfft3=18, 
 /
EOF

touch dummy
$DO_PARALLEL $sander -O -i mdin -c eq12.x -o mdout.tip4p_nve < dummy || goto error

if ( `basename $sander` == "pmemd.mic_offload.MPI" ) then
../dacdif -r 1.0e-06 mdout.tip4p_nve.save mdout.tip4p_nve
else
../dacdif mdout.tip4p_nve.save mdout.tip4p_nve
endif
/bin/rm -f mdin restrt mdinfo dummy
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
