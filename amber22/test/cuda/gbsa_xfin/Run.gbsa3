#!/bin/csh -f
#TEST-PROGRAM pmemd.cuda
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

#$1 = PREC_MODEL
#$2 = NETCDF

if( ! $?DO_PARALLEL ) then
  setenv DO_PARALLEL " "
  if( $?TESTsander ) then
      set sander = $TESTsander
  else
      set sander = ${AMBERHOME}/bin/pmemd.cuda_$1
  endif
else
  if( $?TESTsander ) then
      set sander = $TESTsander
  else
      set sander = ${AMBERHOME}/bin/pmemd.cuda_$1.MPI
  endif
endif

cat > gbsa3in <<EOF
 test of gbsa3 dynamics
 &cntrl
   nstlim=10, cut=999.0, igb=1,
   ntpr=1, ntwr=1000, ntt=0,
   ntc=2, ntf=2, tol=0.000001,
   ntx=5, irest=1, ntb=0, 
   offset=0.09, gbsa=3, ig=71277,
 /
EOF

set output = mdout.gbsa3_out

set output_save = $output.GPU_$1

touch dummy
$DO_PARALLEL $sander -O -i gbsa3in -r restrt -c md7.x -o $output -x mdcrd -p prmtop < dummy || goto error

#Use different dacdif tolerances based on precision model.
if ( "$1" == "DPFP" ) then
  #7 sig figs
  ../../dacdif -r 1.0e-7 $output_save $output
else
  #4 sig figs
  ../../dacdif -r 1.0e-4 $output_save $output
endif

/bin/rm -f mdin logfile mdinfo dummy restrt mdcrd
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)










