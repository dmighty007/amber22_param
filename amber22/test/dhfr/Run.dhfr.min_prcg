#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/sander"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > gbin <<EOF
 short minimization
 &cntrl
   ntx=5, irest=1,
   ntc=2, ntf=2, tol=0.0000001, 
   imin=1, maxcyc=5, ntmin=3,
   ntpr=1, ntwr=10000, 
 /
 &lmod
   xmin_method = "PRCG",
 /
EOF

set output = mdout.dhfr.min_prcg

touch dummy
$DO_PARALLEL $TESTsander -O -i gbin -c md12.x -o $output < dummy || goto error

../dacdif $output.save $output
/bin/rm -f gbin restrt mdinfo dummy
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
