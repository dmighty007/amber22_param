#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
   set sander = $TESTsander
endif

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

if ( "$1" == "" ) then
  set output = mdout.gb.amd1
else
  set output = pmemd.gb.amd1
endif

cat > gbin <<EOF
  test of gen. Born, on a stretch of A-RNA AMD=1
 &cntrl                                                                        
   nstlim=10, cut=9999.0, igb=1, saltcon=0.1,
   ntpr=1, ntwx=1, ntwr=1, ntt=0,
   ntx=5, irest=1, 
   ntc=2, ntf=2, tol=0.000001,
   dt=0.001, ntb=0,
   iamd=1,
   ethreshp=-3962,alphap=128,
   ethreshd=490,alphad=14, ig=71277,
 /
EOF

$DO_PARALLEL $sander -O -i gbin -p prmtop -c md4.x -o $output < /dev/null || goto error

if ( `basename $sander` == "pmemd.mic_offload.MPI" ) then
../../dacdif -r 1.0e-07 $output.save $output
else
../../dacdif $output.save $output
endif
/bin/rm -f gbin restrt mdinfo amd.log mdcrd
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









