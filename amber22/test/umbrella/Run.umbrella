#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/sander"

if( ! $?TESTsander ) then
   set sander = "${AMBERHOME}/bin/sander"
else
   set sander = "$TESTsander"
endif

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
else
  set numprocs=`echo $DO_PARALLEL | awk -f ../numprocs.awk `
  if ( "$numprocs" > 19 ) then
      echo " DO_PARALLEL set to $DO_PARALLEL"
      goto toomany
  else if ( $?MP_PROCS)then
      if ($MP_PROCS > 19)then
#               --- For IBM AIX ---
          echo "    MP_PROCS set to "$MP_PROCS
          goto toomany
      endif
  endif
endif

cat > gbin <<EOF
 test of umbrella sampling of a chi torsion angle
 &cntrl                                                                        
   nstlim=10, cut=99.0, igb=1, saltcon=0.1,
   ntpr=1, ntwr=100000, ntt=3, gamma_ln=0.0,
   ntx=5, irest=1, 
   ntc=2, ntf=2, tol=0.000001,
   dt=0.001, ntb=0,
   nmropt=1,
 /
 &wt type='DUMPFREQ', istep1=1  /
 &wt type='END'   /
DISANG=chi.RST
DUMPAVE=chi_vs_t
LISTIN=POUT
LISTOUT=POUT
EOF

$DO_PARALLEL $TESTsander -O -i gbin -c md4.x  -o mdout.umbrella < /dev/null || goto error

../dacdif mdout.umbrella.save mdout.umbrella
../dacdif chi_vs_t.save chi_vs_t
/bin/rm -f gbin restrt mdinfo
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)

toomany:
    echo " too many processors for this test, exiting (Max = 19)"
    echo "============================================================"
    exit(0)










