#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/sander"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
else
    echo "This test not set up for parallel"
    echo " cannot run in parallel with pbsa"
    exit 0
endif

set output = mdout.polyAT.min

cat > min.in <<EOF
 test of pbsa
 &cntrl
   ntx=1, imin=1,
   ntb=0, ipb=1, inp=0
 /
 &pb
   npbverb=1, istrng=0, epsout=80.0, epsin=1.0, space=0.5,
   accept=1e-9, sprob=0.6, dprob=0.6, radiopt=0, fillratio=1.5, bcopt=7,
   eneopt=2, frcopt=2, cutnb=12, smoothopt=1,
   nfocus=1, cutsa=11, arcres=0.0625,
   offx=.24850459303567613757, offy=.15836817529831843012, offz=.09795678579058198797,
 /
EOF

touch dummy
$DO_PARALLEL $TESTsander -O -i min.in -o $output < dummy || goto error

   echo "checking polyAT force"
   if ( ! -f force.dat || ! -f force.save ) then
      echo "FAILED: file not exist"
      exit(1)
   endif
#  if ( `diff force.dat force.save | cat | wc -c` != 0 ) then
#     echo "FAILED: polyAT force"
#     exit(0)
#  else
#     echo "PASSED" 
#  endif 
#  rm *.dat

#../../../dacdif $output.save $output
../../../dacdif -a 1e-4 force.save force.dat
/bin/rm -f min.in restrt mdinfo dummy $output
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)







