#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
   set sander = $TESTsander
endif

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin <<EOF
min H

 &cntrl
  dielc =1.0, 
  cut    = 8.0,     nsnb   = 5,      ibelly = 0,
  ntx    = 5,       irest  = 1,      ipol = 1,
  ntf    = 2,       ntc    = 2,       tol    = 0.0000005,
  ntb    = 1,       ntp    = 0,       temp0 = 300.0, tautp = 0.2,
 nstlim = 10,    dt = 0.001,  maxcyc = 10,      imin = 0,  ntmin = 2,
  ntpr   = 1,     ntwx = 0, ntt = 0, ntr = 0,
           jfastw = 0, ntave = 100,
 /
 &ewald
    ew_type = 0, verbose=0,
    vdwmeth=1, maxiter=10, indmeth = 3, diptau = 1.0, irstdip=1,
    frameon=1,
 /
restraints on dna
2.0
RES  1     25
END
END
EOF

$DO_PARALLEL $sander -O -i mdin -o mdout.dna_pol < /dev/null || goto error

../dacdif mdout.dna_pol.save mdout.dna_pol
/bin/rm -f mdin restrt mdinfo rstdip
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
