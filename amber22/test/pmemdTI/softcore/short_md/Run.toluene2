#!/bin/csh -f
#TEST-PROGRAM pmemd
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented
 
if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/pmemd"
 
if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin.0 <<EOF
NPT production
 &cntrl
  imin = 0,	ntx = 1,	irest = 0,
  ntpr = 1,	ntwr = 0,	ntwx = 0, ioutfm=0, ntxo=1,
  ntf = 1,	ntc = 2,
  ntb = 2,	cut = 9.0,
  nstlim = 10,	dt = 0.002,
  tempi = 100.0,	vrand = 5,
  temp0 = 300.0,	ntt = 2,
  ntp = 1,	pres0 = 1.0,	taup = 2.0,
  icfe=1,       clambda = 0.50000,
  ifsc=1,       timask1=':1', timask2='',
                scmask1=':1', scmask2='',
  logdvdl=0, ig=71277,
 /
EOF

cat > groupfile <<EOF
-O -i mdin.0 -o out2.0 -p ../prmtop.0 -c ../rst.0 -r restrt2.0
EOF

$DO_PARALLEL $TESTsander `cat groupfile` < /dev/null || goto error

if ( `basename $TESTsander` == "pmemd.mic_offload.MPI" ) then
../../../dacdif -r 1.0e-07 out2.0.save out2.0
else
../../../dacdif out2.0.save out2.0
endif
../../../dacdif -r 1.e-5 restrt2.0.save restrt2.0

/bin/rm -f mdin.0 mdinfo groupfile logfile
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)







