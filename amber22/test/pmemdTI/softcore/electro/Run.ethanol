#!/bin/csh -f
#TEST-PROGRAM pmemd
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented
 
if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/pmemd"
 
if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin.0 <<EOF
NPT run V0
 &cntrl
  imin = 0, ntx = 5, irest = 1,
  ntpr = 1, ntwr = 0, ntwx = 0, ioutfm=0, ntxo=1,
  ntf = 1, ntc = 2, cut = 9.0,
  ntb = 2, nstlim = 20, dt = 0.002,
  temp0 = 298, ntt = 3, gamma_ln = 2,
  ntp = 1, pres0 = 1.0, taup = 2,
  icfe = 1, clambda = 0.30,
  ifsc = 1,
  timask1 = ':1',scmask1=':1',
  timask2 = '',scmask2='',
  scalpha = 0.4,
  scbeta = 8, ig=71277,
 /
EOF

cat > groupfile <<EOF
-O -i mdin.0 -o out.0 -p prmtop.0 -c rst.0 -r restrt.0
EOF

$DO_PARALLEL $TESTsander `cat groupfile` < /dev/null || goto error

../../../dacdif out.0.save out.0
../../../dacdif -r 1.e-5 restrt.0.save restrt.0

/bin/rm -f mdin.0 mdinfo groupfile logfile
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)







