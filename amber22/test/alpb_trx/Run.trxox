#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/sander"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > gbin <<EOF
 test of ALPB. 
 &cntrl                                                                        
   imin=1, maxcyc=5,
   alpb=1, Arad=16.444
   cut=99.0, igb=5, saltcon=0.2, gbsa=1,
   ntpr=1, ntx=1, ntb=0, ig=71277,
 /
EOF

set output = mdout.trxox

$DO_PARALLEL $TESTsander -O -i gbin -c trxox.2.4ns.x  -o $output || goto error

../dacdif $output.save $output
/bin/rm -f gbin restrt mdinfo
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









