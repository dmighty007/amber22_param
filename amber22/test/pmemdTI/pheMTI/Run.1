#!/bin/csh -f
#TEST-PROGRAM pmemd
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/pmemd"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin <<EOF
  test of toluene -> no charge
 &cntrl
   nstlim=10, ntpr=1, cut=9.0, ntx=5, irest=1,
   ntc=2, ntf=2, tol=0.000001, ig=71277,
 /
EOF

touch dummy
$DO_PARALLEL $TESTsander -O -i mdin -p prmtop.1 -o out.1 < dummy || goto error

../../dacdif out.1.save out.1
/bin/rm -f mdin restrt mdinfo dummy
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









