#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented
#

set mpi = 1
set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
   set sander = "$TESTsander"
endif

if( ! $?DO_PARALLEL ) then
  setenv DO_PARALLEL " "
  set mpi = 0
else
  set numprocs=`echo $DO_PARALLEL | awk -f ../numprocs.awk `
  if ( "$numprocs" > 19 ) then
      echo " DO_PARALLEL set to $DO_PARALLEL"
      goto toomany
  else if ( $?MP_PROCS)then
      if ($MP_PROCS > 19)then
#               --- For IBM AIX ---
          echo "    MP_PROCS set to "$MP_PROCS
          goto toomany
      endif
  endif
endif

cat <<EOF >mdin
 just get final values
 &cntrl
    ntx=1, ntb=0,
    pencut= 0.01, nmropt=2, ipnlty=1,
    imin=1, maxcyc=1,  
    ntc=1, ntf=1, cut=9.0,
    mxsub=10, tausw=0.101, 
 /
 
 &wt type='NOESY', istep1=0,istep2=500,value1=1.0,
            value2=1.0,  /
 
 &wt type='END'  /
NOESY=NXP.8May95
DISANG=RST.16sept93
LISTOUT=POUT
EOF

touch dummy
$DO_PARALLEL $sander -O -i mdin -c 01.ann10.xyz -o noesy.out < dummy || goto error

if( $mpi ) then
	../dacdif noesy.out.mpi.save noesy.out
else
	../dacdif noesy.out.save noesy.out
endif

/bin/rm -f mdinfo restrt mdin dummy
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)

toomany:
    echo " too many processors for this test, exiting (Max = 19)"
    echo "============================================================"
    exit(0)










