#!/bin/csh -f
#TEST-PROGRAM pmemd.cuda
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

#$1 = PREC_MODEL
#$2 = NETCDF

if( ! $?DO_PARALLEL ) then
  setenv DO_PARALLEL " "
  if( $?TESTsander ) then
      set sander = $TESTsander
  else
      set sander = ${AMBERHOME}/bin/pmemd.cuda_$1
  endif
else
  if( $?TESTsander ) then
      set sander = $TESTsander
  else
      set sander = ${AMBERHOME}/bin/pmemd.cuda_$1.MPI
  endif
endif

cat > mdin <<EOF
 short md, nve ensemble
 &cntrl
   ntx=7, irest=1,
   ntc=2, ntf=2, tol=0.0000001, 
   nstlim=20, ntt=0, 
   ntpr=1, ntwr=10000, 
   dt=0.001,ntr=1, ig=71277,
 /
 &ewald
   nfft1 = 72, nfft2 = 60, nfft3 = 60, netfrc=0,
 /
restraints 1
25.0
RES 2 5
END
restraints 2
20.0
RES 9 11
END
restraints 3
11.5
RES 50 50
END
restrains 4
97.1
RES 17 22
END
END
EOF

set output = mdout.dhfr.ntr1

set output_save = $output.GPU_$1

touch dummy
$DO_PARALLEL $sander -O -o $output -c md12.x -r restrt -x mdcrd -ref md12.x < dummy || goto error
#Use different dacdif tolerances based on precision model.
if ( "$1" == "DPFP" ) then
  #7 sig figs
  ../../dacdif -r 1.0e-7 $output_save $output
else
  #3 sig figs
  ../../dacdif -r 1.4e-3 $output_save $output
endif

/bin/rm -f mdin logfile mdinfo dummy restrt mdcrd
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)










