#
# assumed that most often users will want to use HID
#
NHIS = NHID
HIS = HID
CHIS = CHID

# Aliases
alias 	a	alias
a	q	quit
a	exit	quit
a	?	help
a	e	edit
a	s 	source
a	seq 	sequence
# Aliases abbreviated w/ verbs 1st
a	l 	loadOff
a	lp 	loadPdb
a	so 	saveOff
a	sop 	saveOffParm
a	sap 	saveAmberParm
a	sp 	savePdb
a	ap 	loadAmberParams
a	lap 	loadAmberPrep
a	ai 	addIons
