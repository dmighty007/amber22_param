#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/sander"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin <<EOF
 short minimization
 &cntrl
   ntx=1, irest=0,
   imin=1,ntf=2,ntc=2,
   maxcyc=20,ncyc=10,ntb=1,
   ntpr=1,cut=9.0,tol=0.0000001, ig=71277,
 /
 &ewald
  ew_coeff=0.340,nfft1=96,nfft2=80,nfft3=64,order=4,vdwmeth=0,
 /
EOF

set output = mdout.dhfr_charmm.min

touch dummy
$DO_PARALLEL $TESTsander -O -i mdin -c inpcrd -o $output < dummy || goto error

../../../dacdif $output.save $output
/bin/rm -f mdin restrt mdinfo dummy
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)







