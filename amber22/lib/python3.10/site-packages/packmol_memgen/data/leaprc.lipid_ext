    { "Ha"  "H" "sp3" }
    { "Hp"  "H" "sp3" }
    { "Hc"  "H" "sp3" }
    { "Ho"  "H" "sp3" }
    { "HW"  "H" "sp3" }
    { "Ng"  "N" "sp2" }
    { "NT"  "N" "sp3" }
    { "N3"  "N" "sp3" }
    { "Oh"  "O" "sp3" }
    { "Os"  "O" "sp3" }
