#!/bin/sh

. ../../program_error.sh

if [ -z "$parmed" ]; then

   parmed="$AMBERHOME/bin/parmed"
fi

output=parmed.out
prmtop=znf.prmtop

cat > parmed.in << EOF
parm 4lzt.parm7
addPDB 4lzt.pdb
outparm 4lzt_pdb.parm7
EOF

$parmed -On -i parmed.in > $output 2>&1 || error

../../dacdif 4lzt_pdb.parm7.save 4lzt_pdb.parm7

/bin/rm -f $output parmed.in

exit 0
