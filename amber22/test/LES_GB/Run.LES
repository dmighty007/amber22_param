#!/bin/csh -f
#TEST-PROGRAM sander.LES
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

echo "SANDER: LES+GB"

if( ! $?TESTsanderLES ) set TESTsanderLES = "${AMBERHOME}/bin/sander.LES"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
else
        set numprocs=`echo $DO_PARALLEL | awk -f ../numprocs.awk `
        if ( "$numprocs" > 4 ) then
            echo " DO_PARALLEL set to $DO_PARALLEL"
            goto toomany
        else if ( $?MP_PROCS)then
            if ($MP_PROCS > 4)then
#               --- For IBM AIX ---
                echo "    MP_PROCS set to "$MP_PROCS
                goto toomany
            endif
        endif
endif

set output = les.gb1.diffcrd.out
touch dummy
echo "1: GB/LES GB1 diffcoords"
$DO_PARALLEL $TESTsanderLES \
-O \
-i ld.gb1.in \
-o $output \
-p les.prm \
-c les.diffcoords.r \
-x les.x  \
-inf lesinfo \
-r les.r < dummy || goto error

../dacdif $output.save $output
/bin/rm -f les.x lesinfo les.r dummy

set output = les.gb1.samecrd.out
touch dummy
echo "1: GB/LES GB1 samecoords"
$DO_PARALLEL $TESTsanderLES \
-O \
-i md.gb1.in \
-o $output \
-p les.alt.prm \
-c les.samecoords.r \
-x les.x  \
-inf lesinfo \
-r les.r < dummy || goto error

../dacdif $output.save $output
/bin/rm -f les.x lesinfo les.r dummy

set output = les.gb5.samecrd.out
touch dummy
echo "1: GB/LES GB5 samecoords"
$DO_PARALLEL $TESTsanderLES \
-O \
-i md.gb5.in \
-o $output \
-p les.alt.prm \
-c les.samecoords.r \
-x les.x  \
-inf lesinfo \
-r les.r < dummy || goto error

../dacdif $output.save $output
/bin/rm -f les.x lesinfo les.r dummy

set output = les.gb7.samecrd.out
touch dummy
echo "1: GB/LES GB7 samecoords"
$DO_PARALLEL $TESTsanderLES \
-O \
-i md.gb7.in \
-o $output \
-p les.alt.prm \
-c les.samecoords.r \
-x les.x  \
-inf lesinfo \
-r les.r < dummy || goto error

../dacdif $output.save $output
/bin/rm -f les.x lesinfo les.r dummy

set output = les.gb7.rdt.out
touch dummy
echo "1: GB/LES GB7 diffcoords w/RDT"
$DO_PARALLEL $TESTsanderLES \
-O \
-i md.gb7.rdt.in \
-o $output \
-p les.prm \
-c les.diffcoords.r \
-x les.x  \
-inf lesinfo \
-r les.r < dummy || goto error

../dacdif $output.save $output
/bin/rm -f les.x lesinfo les.r dummy

exit(0)

error:
echo "  ${0}:  Program error"
exit(1)

toomany:
    echo " too many processors for this test, exiting"
    echo "============================================================"
    exit(0)










