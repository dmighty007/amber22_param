#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "${AMBERHOME}/bin/sander"
if( $?TESTsander ) then
   set sander = $TESTsander
endif

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

set output = mdout.alp

cat > gbin <<EOF
Equilibration of unit cell 
 &cntrl
   IREST=1,
   NTX=5,  iwrap=0,
   NTXO=1, NTPR=1, 
   NTF=2, NTB=2,
   NSTLIM=20, DT=0.001,
   NTT=0, 
   NTC=2, NTP=1, ig=71277,
 /
 &ewald
  nfft1=72, nfft2=72, nfft3=90
 /
EOF

touch dummy
$DO_PARALLEL $sander -O -i gbin -o $output < dummy || goto error

if ( `basename $sander` == "pmemd.mic_offload.MPI" ) then
../dacdif -r 1.0e-5 $output.save $output
else
../dacdif $output.save $output
endif
/bin/rm -f gbin restrt mdinfo dummy
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









