#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/sander"

if ! { ../checkrismunsupported.sh $TESTsander } exit 0

cat > mdin <<EOF
 simple check for phosphate
 &cntrl
    ntx=1, ntpr=1, ntwx=0,
    imin=0,nstlim=0,
    ntp=0,
    ntc=3,ntf=3,
    ntb=0,
    nrespa=1,
    cut=12,
    irism=1,
/
&rism
    buffer = 12d0
    npropagate=0
    closure="psen"
    closureOrder=1
    gfCorrection = 1
    pcplusCorrection = 1
    uccoeff=-0.149818,-3.2114559,-0.00053163,0.0103954
    treeDCF=.false.
    treeTCF=.false.
    treeCoulomb=.false.
    asympKSpaceTolerance = 0
    ljTolerance = 0
    volfmt="dx"
/
EOF

$TESTsander -O -i mdin -o ph1-nacl.pse1.out \
   -p ph1.top -c ph1.x -xvv tip3p-nacl.xvv -guv guv.dx \
   || goto error

../../../AmberTools/test/nab/checkrism3d -err 1e-6 ph1-nacl.pse1.out.save ph1-nacl.pse1.out
/bin/rm -f mdin mdinfo restrt
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









