#!/bin/csh -f
#TEST-PROGRAM pmemd
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

if( ! $?TESTsander ) set TESTsander = "${AMBERHOME}/bin/pmemd"

if( ! $?DO_PARALLEL ) then
        setenv DO_PARALLEL " "
endif

cat > mdin <<EOF
  test of toluene -> no charge
 &cntrl
   nstlim=10, ntpr=1, cut=9.0, ntx=5, irest=1,
   icfe=1, clambda=0.0,
   timask1=":1",
   timask2=":2",
   crgmask=":2",
   ntc=2, ntf=2, tol=0.000001, ig=71277,
 /
EOF

cat > groups <<EOF
-O -i mdin -p ti.prmtop -o pheMTI0.out -c inpcrd3
EOF

$DO_PARALLEL $TESTsander `cat groups` < /dev/null || goto error

../../dacdif pheMTI0.out.save pheMTI0.out
/bin/rm -f mdin restrt mdinfo dummy groups
exit(0)

error:
echo "  ${0}:  Program error"
exit(1)









